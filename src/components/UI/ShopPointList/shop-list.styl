@import "~@/style/import"
.print-point-list
  margin: 0 auto
  width: 880px
  font-size: 12px
  line-height: 1.5
  text-align: center
  color: #666

.point-detial-title
  margin: 4px auto
  width: 840px
  text-align: start
  .back
    border: none
    border-radius: 4px
    padding: 4px 10px
    font-size: 12px
    vertical-align: middle
    color: shallow-text-gray
    background: transparent
    transition: all .2s ease-out
    cursor: pointer
    &:hover
      background: #eee
  .tip
    margin-left: 10px
    border-left: 1px solid #ddd
    padding-left: 10px
    font-size: 16px
    vertical-align: middle
    color: #096
