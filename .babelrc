{
  "presets": [
    "es2015",
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-optional-chaining",
    "transform-runtime"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
